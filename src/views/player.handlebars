
	{{#if embedded}}
	<style type="text/css">
	body, html {
		overflow-x: hidden;
	}
	</style>
	{{/if}}

	<div class="log"></div>

	<div class="header-bar">
		<div class="brand">Unplugd.io</div>
		<div class="online">20 Pople Online</div>
	</div>

	<div class="player-container">
	    <audio controls>
	      <source src="http://50.7.71.27:9535/live" />
	    </audio>
	</div>

	<div class="player-controls-wrapper">
		<div class="player-controls">
			<div class="controls">
				<div class="button play-button"></div>
			</div>
			<div class="status"></div>
		</div>
	</div>

	<div class="playlist-container scrollable">

	</div>

	<script id="playlist-item-template" type="text/x-handlebars-template">
		<div class="playlist-item">
			<div class="backdrop"><img src="\{{imgSrc}}" /></div>
			<div class="top-label"></div>
			<div class="bottom-label-wrapper">
				<div class="bottom-label">\{{title}}</div>
			</div>
		</div>
	</script>

